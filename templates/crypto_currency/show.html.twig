{% extends 'base.html.twig' %}

{% block title %}CryptoCurrency{% endblock %}

{% block body %}
    <h1>CryptoCurrency</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ crypto_currency.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ crypto_currency.name }}</td>
            </tr>
            <tr>
                <th>Symbol</th>
                <td>{{ crypto_currency.symbol }}</td>
            </tr>
            <tr>
                <th>Slug</th>
                <td>{{ crypto_currency.slug }}</td>
            </tr>
            <tr>
                <th>CapId</th>
                <td>{{ crypto_currency.capId }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_crypto_currency_index') }}">back to list</a>

    <a href="{{ path('app_crypto_currency_edit', {'id': crypto_currency.id}) }}">edit</a>

    {{ include('crypto_currency/_delete_form.html.twig') }}
{% endblock %}
